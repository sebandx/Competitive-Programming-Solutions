import java.util.*;
public class AC7P1 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int t = input.nextInt();
		
		for (int i = 0; i < t; i++) {
			int n = input.nextInt();
			if (n == 2) {
				System.out.println(2);
			}
			else {
				System.out.println(n - 1);
			}
		}
	}
}
