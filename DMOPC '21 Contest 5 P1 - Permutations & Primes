#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

int main() {
    ll n; cin >> n;
    if (n == 1) {
        cout << "1" << "\n";
    } else if (n <= 4) {
        cout << "-1" << "\n";
    } else {
        for (int i = 2; i <= n; i += 2) {
            if (i != 4) {
                cout << i << " ";
            }
        }
        cout << 4 << " " << 5;
        for (int i = 1; i <= n; i += 2) {
            if (i != 5) {
                cout << " " << i;

            }
        }

        cout << "\n";



    }

    return 0;
}
